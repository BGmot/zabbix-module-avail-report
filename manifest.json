{
  "manifest_version": 1.0,
  "id": "availability_report",
  "name": "Availability report",
  "version": "1.0.2",
  "namespace": "BGmotAR",
  "author": "Evgeny Yurchenko",
  "url": "https://bgmot.com",
  "description": "Availability report improvementes",
  "actions": {
    "availreport.view": {
      "class": "CControllerBGAvailReportView",
      "view": "module.reports.availreport.view",
      "layout": "layout.htmlpage"
    },
    "availreport.view.refresh": {
      "class": "CControllerBGAvailReportViewRefresh",
      "view": "module.reports.availreport.view.refresh",
      "layout": "layout.json"
    },
    "availreport.view.csv": {
      "class": "CControllerBGAvailReportView",
      "view": "module.reports.availreport.view",
      "layout": "layout.csv"
    },
    "tabfilter.profile.update": {
      "class": "CControllerBGTabFilterProfileUpdate",
      "view": null,
      "layout": "layout.json"
    }
  }
}
